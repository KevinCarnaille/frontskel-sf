<!DOCTYPE html>
<html>
<head>

    <meta charset="UTF-8" />
    <title>{% block title %}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="title" content="{% block title_meta %}{% endblock %}" />
    <meta name="keywords" content="{% block keywords_meta %}{% endblock %}" />
    <meta name="description" content="{% block description_meta %}{% endblock %}" />
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />


    {% block stylesheets %}
        {% stylesheets
        'bundles/mystatic/css/knacss/knacss.less'
        'bundles/mystatic/css/font-awesome/font-awesome.less'
        'bundles/mystatic/css/global.less'
        'bundles/mystatic/css/responsive.less'
        'bundles/mystatic/fonts/stylesheet.less'
        filter='uglifycss' filter='cssrewrite' %}

                <link rel="stylesheet" href="{{ asset_url }}">

        {% endstylesheets %}

    {% endblock %}
    {#<link rel="stylesheet" href="{{ asset('bundles/mystatic/fonts/stylesheet.less') }}" />#}




    <!--[if lt IE 9]>

    <script type="text/javascript" src="{{ asset('bundles/mystatic/js/html5shiv.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/mystatic/js/placeholder.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/mystatic/js/respond.min.js') }}"></script>


    <![endif]-->


</head>
<body id="{% block id_body %}default{% endblock %}">


<header id="main_header" class="white">
    {% block header %}
    {% endblock %}
</header>

{% block body %}
{% endblock %}


<footer id="main_footer">
    {% block footer %}
    {% endblock %}
</footer>


{% block javascripts %}
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>

    {% javascripts '@MyStaticBundle/Resources/public/js/global.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>
